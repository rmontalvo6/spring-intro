package pe.peru;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.stereotype.Component;

@Component
public class SpringBean {
	
	private static final Logger log = LoggerFactory.getLogger(OtroSpringBean.class);

	public SpringBean() {
		log.info("Crea Una Instancia de OTRO BEAN"+this.hashCode());
		
	}
}
